<form action="/dinosaurs" method="GET"> <% //get method in forms adds query string to route %> 
    <label for="nameFilter">Filter by Name</label>
    <input type="text" id="nameFilter" name="nameFilter">
    <input type="submit">
</form>

<ul>
    <% dinoData.forEach( (dino, index) => { %>
        <li>
            <%= dino.name %> is a <%= dino.type %>
            <form action="/dinosaurs/<%= index %>/?_method=DELETE" method="POST">
                <input type="submit" value="Delete">
            </form>
        </li>
    <% }) %> 
</ul>